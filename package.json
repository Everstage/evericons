{
  "name": "heroicons",
  "version": "2.0.17",
  "license": "MIT",
  "files": [
    "static/",
    "README.md"
  ],
  "scripts": {
    "prepublishOnly": "npm run build",
    "lint": "node ./scripts/verify-names.js",
    "prebuild": "rimraf ./static ./react/static ./optimized/static",
    "build": "npm run build-static && npm run build-react",
    "build-react": "node ./scripts/build.js react",
    "build-static": "rimraf ./optimized/static && svgo --config=svgo.static.yaml -f ./src/static -o ./optimized/static --pretty --indent=2 && mkdir -p ./static && cp -R ./optimized/static/ ./static",
    "release-channel": "node ./scripts/release-channel.js",
    "release-notes": "node ./scripts/release-notes.js"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/plugin-transform-react-jsx": "^7.12.12",
    "@svgr/core": "^5.5.0",
    "camelcase": "^6.0.0",
    "prettier": "^2.8.7",
    "rimraf": "^3.0.2",
    "svgo": "^1.3.2"
  }
}