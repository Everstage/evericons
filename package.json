{
  "name": "evericons",
  "version": "2.0.17",
  "license": "MIT",
  "files": [
    "static/",
    "react/",
    "README.md"
  ],
  "scripts": {
    "prepublishOnly": "npm run build",
    "lint": "node ./scripts/verify-names.js",
    "prebuild": "rimraf ./static ./react/static ./optimized/static",
    "build": "npm run build-static && npm run build-react",
    "build-react": "node ./scripts/build.js react",
    "build-static": "rimraf ./optimized/static && svgo --config=svgo.static.yaml -f ./src/static -o ./optimized/static --pretty --indent=2 && mkdir -p ./static && cp -R ./optimized/static/ ./static"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/plugin-transform-react-jsx": "^7.12.12",
    "@svgr/core": "^5.5.0",
    "camelcase": "^6.0.0",
    "prettier": "^2.8.7",
    "rimraf": "^3.0.2",
    "svgo": "^1.3.2"
  },
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./react/index.esm.js",
      "require": "./react/index.js"
    },
    "./react/static": {
      "types": "./react/static/index.d.ts",
      "import": "./react/static/esm/index.js",
      "require": "./react/static/index.js"
    },
    "./react/static/index": {
      "default": "./react/static/index.js"
    },
    "./react/static/index.js": {
      "default": "./react/static/index.js"
    },
    "./react/static/*": {
      "types": "./react/static/*.d.ts",
      "import": "./react/static/esm/*.js",
      "require": "./react/static/*.js"
    },
    "./react/static/*.js": {
      "types": "./react/static/*.d.ts",
      "import": "./react/static/esm/*.js",
      "require": "./react/static/*.js"
    },
    "./react/static/esm/*": {
      "types": "./react/static/*.d.ts",
      "import": "./react/static/esm/*.js"
    },
    "./react/static/esm/*.js": {
      "types": "./react/static/*.d.ts",
      "import": "./react/static/esm/*.js"
    }
  },
  "peerDependencies": {
    "react": ">= 16"
  }
}