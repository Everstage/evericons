{
  "name": "evericons",
  "version": "0.0.1",
  "license": "MIT",
  "files": [
    "react/",
    "README.md"
  ],
  "scripts": {
    "prepublishOnly": "npm run build",
    "prebuild": "rimraf ./static ./optimized/static",
    "build": "npm run build-static && npm run build-react",
    "build-react": "node ./scripts/build.js react",
    "build-static": "rimraf ./optimized/static && svgo --config=svgo.static.yaml -f ./src/static -o ./optimized/static --pretty --indent=2 && mkdir -p ./static",
    "prepare": "npm run build"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/plugin-transform-react-jsx": "^7.12.12",
    "@svgr/core": "^5.5.0",
    "camelcase": "^6.0.0",
    "prettier": "^2.8.7",
    "rimraf": "^3.0.2",
    "svgo": "^1.3.2"
  },
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./index.esm.js",
      "require": "./index.js"
    },
    "./package.json": {
      "default": "./package.json"
    },
    "./static": {
      "types": "./static/index.d.ts",
      "import": "./static/esm/index.js",
      "require": "./static/index.js"
    },
    "./static/*": {
      "types": "./static/*.d.ts",
      "import": "./static/esm/*.js",
      "require": "./static/*.js"
    },
    "./static/*.js": {
      "types": "./static/*.d.ts",
      "import": "./static/esm/*.js",
      "require": "./static/*.js"
    },
    "./static/esm/*": {
      "types": "./static/*.d.ts",
      "import": "./static/esm/*.js"
    },
    "./static/esm/*.js": {
      "types": "./static/*.d.ts",
      "import": "./static/esm/*.js"
    }
  },
  "peerDependencies": {
    "react": ">= 16"
  }
}
