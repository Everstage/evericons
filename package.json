{
  "name": "evericons",
  "version": "0.0.1",
  "license": "MIT",
  "type": "module",
  "files": [
    "outlined/",
    "duocolor/",
    "duotone/",
    "solid/",
    "lotties/",
    "README.md"
  ],
  "scripts": {
    "prebuild": "rimraf ./outlined ./duocolor ./duotone ./solid ./lotties ./optimized/outlined ./optimized/duocolor ./optimized/duotone ./optimized/solid",
    "build": "npm run build-outlined && npm run build-duocolor && npm run build-duotone && npm run build-solid && npm run build-lotties && node ./scripts/build.js",
    "build-outlined": "rimraf ./optimized/outlined && svgo --config ./svgo.outlined.config.js -f ./src/outlined -o ./optimized/outlined && mkdir -p ./outlined",
    "build-duocolor": "rimraf ./optimized/duocolor && svgo --config ./svgo.outlined.config.js -f ./src/duocolor -o ./optimized/duocolor && mkdir -p ./duocolor",
    "build-duotone": "rimraf ./optimized/duotone && svgo --config ./svgo.outlined.config.js -f ./src/duotone -o ./optimized/duotone && mkdir -p ./duotone",
    "build-solid": "rimraf ./optimized/solid && svgo --config ./svgo.solid.config.js -f ./src/solid -o ./optimized/solid && mkdir -p ./solid",
    "build-lotties": "mkdir -p ./lotties && cp ./src/lotties/*.json ./lotties",
    "prepare": "npm run build"
  },
  "devDependencies": {
    "@babel/core": "^7.21.5",
    "@babel/plugin-transform-react-jsx": "^7.21.5",
    "@svgr/core": "^7.0.0",
    "camelcase": "^7.0.1",
    "prettier": "^2.8.8",
    "rimraf": "^5.0.0",
    "svgo": "^3.0.2"
  },
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./index.esm.js",
      "require": "./index.js"
    },
    "./package.json": {
      "default": "./package.json"
    },
    "./lotties": {
      "types": "./lotties/index.d.ts",
      "import": "./lotties/esm/index.js",
      "require": "./lotties/index.js"
    },
    "./lotties/*": {
      "types": "./lotties/*.d.ts",
      "import": "./lotties/esm/*.js",
      "require": "./lotties/*.js"
    },
    "./lotties/*.js": {
      "types": "./lotties/*.d.ts",
      "import": "./lotties/esm/*.js",
      "require": "./lotties/*.js"
    },
    "./lotties/esm/*": {
      "types": "./lotties/*.d.ts",
      "import": "./lotties/esm/*.js"
    },
    "./lotties/esm/*.js": {
      "types": "./lotties/*.d.ts",
      "import": "./lotties/esm/*.js"
    },
    "./outlined": {
      "types": "./outlined/index.d.ts",
      "import": "./outlined/esm/index.js",
      "require": "./outlined/index.js"
    },
    "./outlined/*": {
      "types": "./outlined/*.d.ts",
      "import": "./outlined/esm/*.js",
      "require": "./outlined/*.js"
    },
    "./outlined/*.js": {
      "types": "./outlined/*.d.ts",
      "import": "./outlined/esm/*.js",
      "require": "./outlined/*.js"
    },
    "./outlined/esm/*": {
      "types": "./outlined/*.d.ts",
      "import": "./outlined/esm/*.js"
    },
    "./outlined/esm/*.js": {
      "types": "./outlined/*.d.ts",
      "import": "./outlined/esm/*.js"
    },
    "./duocolor": {
      "types": "./duocolor/index.d.ts",
      "import": "./duocolor/esm/index.js",
      "require": "./duocolor/index.js"
    },
    "./duocolor/*": {
      "types": "./duocolor/*.d.ts",
      "import": "./duocolor/esm/*.js",
      "require": "./duocolor/*.js"
    },
    "./duocolor/*.js": {
      "types": "./duocolor/*.d.ts",
      "import": "./duocolor/esm/*.js",
      "require": "./duocolor/*.js"
    },
    "./duocolor/esm/*": {
      "types": "./duocolor/*.d.ts",
      "import": "./duocolor/esm/*.js"
    },
    "./duocolor/esm/*.js": {
      "types": "./duocolor/*.d.ts",
      "import": "./duocolor/esm/*.js"
    },
    "./duotone": {
      "types": "./duotone/index.d.ts",
      "import": "./duotone/esm/index.js",
      "require": "./duotone/index.js"
    },
    "./duotone/*": {
      "types": "./duotone/*.d.ts",
      "import": "./duotone/esm/*.js",
      "require": "./duotone/*.js"
    },
    "./duotone/*.js": {
      "types": "./duotone/*.d.ts",
      "import": "./duotone/esm/*.js",
      "require": "./duotone/*.js"
    },
    "./duotone/esm/*": {
      "types": "./duotone/*.d.ts",
      "import": "./duotone/esm/*.js"
    },
    "./duotone/esm/*.js": {
      "types": "./duotone/*.d.ts",
      "import": "./duotone/esm/*.js"
    },
    "./solid": {
      "types": "./solid/index.d.ts",
      "import": "./solid/esm/index.js",
      "require": "./solid/index.js"
    },
    "./solid/*": {
      "types": "./solid/*.d.ts",
      "import": "./solid/esm/*.js",
      "require": "./solid/*.js"
    },
    "./solid/*.js": {
      "types": "./solid/*.d.ts",
      "import": "./solid/esm/*.js",
      "require": "./solid/*.js"
    },
    "./solid/esm/*": {
      "types": "./solid/*.d.ts",
      "import": "./solid/esm/*.js"
    },
    "./solid/esm/*.js": {
      "types": "./solid/*.d.ts",
      "import": "./solid/esm/*.js"
    }
  },
  "peerDependencies": {
    "react": ">= 16"
  },
  "dependencies": {
    "@svgr/plugin-jsx": "^7.0.0"
  }
}